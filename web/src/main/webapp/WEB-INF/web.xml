<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <servlet>
     <servlet-name>AQMoniterService</servlet-name>
     <servlet-class>org.savara.sam.web.server.AQMonitorServiceMock</servlet-class>
  </servlet>
 
  <servlet-mapping>
      <servlet-name>AQMoniterService</servlet-name>
      <url-pattern>/ApplicationModule/AQMoniterService</url-pattern>
  </servlet-mapping>
    
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>ApplicationModule.html</welcome-file>
  </welcome-file-list>

</web-app>
